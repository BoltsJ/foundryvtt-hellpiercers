{
  "id": "hellpiercers",
  "title": "HELLPIERCERS: Tactical Harrowing Action",
  "description": "HELL IS A PRISON. BREAK ITS BARS. Tactical Hell-slaying action with base building, overwhelming build options, and map based strategy.",
  "version": "0",
  "compatibility": {
    "minimum": "12",
    "verified": "12",
    "maximum": "12"
  },
  "authors": [{ "name": "Bolts" }],
  "esmodules": ["hellpiercers.mjs"],
  "styles": ["style.css"],
  "packs": [
    { "name": "debugmacros", "path": "packs/debugmacros", "type": "Macro", "label": "Debug Macros" }
  ],
  "languages": [{ "lang": "en", "name": "English", "path": "lang/en.json" }],
  "documentTypes": {
    "Actor": {
      "human": { "htmlFields": ["biography"] },
      "demon": { "htmlFields": ["biography"] },
      "boss": { "htmlFields": ["biography"] },
      "faction": { "htmlFields": ["description"] }
    },
    "Item": {
      "weapon": { "htmlFields": ["description", "ability.description"] },
      "armor": { "htmlFields": ["description", "ability.description"] },
      "class": { "htmlFields": ["description", "active.description", "passive.description"] },
      "gear": { "htmlFields": ["description", "effect"] },
      "equipment": { "htmlFields": ["description"] }
    }
  },
  "socket": false,
  "initiative": null,
  "grid": {
    "distance": 1,
    "units": ""
  },
  "primaryTokenAttribute": "system.health",
  "secondaryTokenAttribute": "",
  "flags": { "hotReload": { "extensions": ["json"], "paths": ["lang"] } },
  "url": "https://github.com/BoltsJ/foundryvtt-hellpiercers",
  "background": "systems/hellpiercers/assets/splash.jpg",
  "manifest": "https://github.com/BoltsJ/foundryvtt-hellpiercers/releases/latest/system.json",
  "download": ""
}
